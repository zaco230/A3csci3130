{
  "rules": {
    ".read": "true",
    ".write": "true",
    "contacts": {
            "$contact": {
                "name": {
                    ".validate": "newData.isString() && newData.val().length > 1 && newData.val().length() < 49"
                },
                "email": {
                    ".validate": "newData.isString() && newData.val().matches(/^[A-Z0-9._%+-]+@[A-Z0-9.-]+\\.[A-Z]{2,4}$/i)"
                },
                "businessNumber": {
                    ".validate": "newData.isNumber() && newData.val().length() == 9"
                },
                "primaryBusiness": {
                    ".validate": "newData.isString() && newData.val().matches(Fisher|Distributor|Processor|Fish Monger)"
                },
                "address": {
                    ".validate": "newData.isString() && newData.val().length() < 50"
                },
                "province": {
                    ".validate": "newDta.isString() && newData.val().matches(AB|BC|MB|NB|NL|NS|NT|NU|ON|PE|QC|SK|YT|' ')"
                }

            }
        }
  }
}